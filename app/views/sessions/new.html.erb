<h1> Log In </h1>
<br>

  
<br>

    <%= form_with(url: login_path, scope: :session, local: true) do |f| %>
        <%= f.label :username %>
        <%= f.text_field :username %></br>

        <%= f.label :password %>
        <%= f.password_field :password %></br>

        <%= f.submit"submit"%>

    <% end %>
